{
    "name": "productivity-panel",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "author": "Remco Stoeten",
    "scripts": {
        "build": "next build",
        "start": "next start",
        "f": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
        "db:check": "drizzle-kit check",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:migrate:drop": "drizzle-kit migrate:drop",
        "db:pull": "drizzle-kit introspect",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "typecheck": "tsc --noEmit",
        "clean": "bun run rm -rf .next node_modules *.tsbuildinfo",
        "rebuild": "bun clean && bun install && bun run f && bun run build"
    },
    "dependencies": {
        "babel-plugin-react-compiler": "^0.0.0-experimental-334f00b-20240725",
        "next": "^15.0.0-rc.0",
        "prettier-plugin-curly": "^0.2.2",
        "prettier-plugin-packagejson": "^2.5.1",
        "react": "^19.0.0-rc-65903583-20240805",
        "react-dom": "^19.0.0-rc-65903583-20240805",
        "sass": "^1.77.8"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "^15.0.0-rc.0",
        "postcss": "^8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}