{
  "cSpell.enable": false,
  "editor.foldingImportsByDefault": true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.autoSave": "onFocusChange",
  "editor.formatOnSaveMode": "file",
  "editor.codeActionsOnSave": {
    // Removed "source.organizeImports" and "source.addMissingImports"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.turbo": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/.astro": true,
    "**/*.lock": true
  },
  "search.exclude": {
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.turbo": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/.astro": true,
    "**/*.lock": true
  },
  "workbench.colorTheme": "SynthWave '84",
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "\"Geist Mono\"",
  "git.enableSmartCommit": true,
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "vscode_custom_css.imports": [
    "file:///home/remcostoeten/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.3/dist/updateHHHandler.js"
  ],
  "github.copilot.advanced": {
    "github.copilot.enable": true,
    "github.copilot.inlineSuggest.enable": true,
    "github.copilot.inlineSuggest.show": true,
    "github.copilot.editor.useTabCompletion": true,
    "github.copilot.editor.acceptSuggestionOnEnter": true,
    "github.copilot.suggestion.maxSuggestions": 225,
    "github.copilot.suggestion.timeout": 100,
    "github.copilot.advanced.experimentalFeatures": true,
    "github.copilot.advanced.loggingLevel": "info",
    "github.copilot.advanced.tracking": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "findUnusedExports.detectCircularImports": true
}
